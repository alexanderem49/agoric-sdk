{
  "name": "@agoric/swing-store",
  "version": "0.9.1",
  "description": "Persistent storage for SwingSet",
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "repository": "https://github.com/Agoric/agoric-sdk",
  "author": "Agoric",
  "license": "Apache-2.0",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error lint:*",
    "lint:types": "tsc",
    "lint:eslint": "eslint ."
  },
  "dependencies": {
    "@agoric/assert": "^0.6.0",
    "@agoric/internal": "^0.3.2",
    "@endo/base64": "^1.0.4",
    "@endo/bundle-source": "^3.2.2",
    "@endo/check-bundle": "^1.0.6",
    "@endo/nat": "^5.0.6",
    "better-sqlite3": "^9.1.1"
  },
  "devDependencies": {
    "@endo/init": "^1.1.1",
    "@types/better-sqlite3": "^7.6.9",
    "ava": "^6.1.2",
    "c8": "^9.1.0",
    "tmp": "^0.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "2m"
  },
  "typeCoverage": {
    "atLeast": 76.3
  }
}
